{
  "id": "warehouse_dashboard",
  "version": 1,
  "type": "page",
  "meta": {
    "title": "Warehouse Dashboard",
    "pageType": "DASHBOARD",
    "cachePolicy": "CACHE_FIRST",
    "scrollable": true,
    "refreshable": true
  },
  "layout": {
    "type": "LIST",
    "areas": {
      "topBar": {
        "id": "top_bar",
        "version": 1,
        "type": "topBar",
        "meta": {
          "title": {
            "text": "Warehouse Dashboard",
            "textSize": "LARGE"
          },
          "showBackButton": false,
          "actions": []
        },
        "properties": {}
      },
      "content": [
        {
          "id": "metrics_grid",
          "version": 1,
          "type": "container",
          "meta": {
            "arrangement": "GRID",
            "columns": 2,
            "spacing": "MEDIUM",
            "scrollable": false,
            "children": [
              {
                "id": "sales_card",
                "version": 1,
                "type": "card",
                "meta": {
                  "fields": [
