                "version": 1,
                "type": "container",
                "meta": {
                  "arrangement": "ROW",
                  "spacing": "MEDIUM",
                  "scrollable": true,
                  "children": [
                    {
                      "id": "create_receipt_action",
                      "version": 1,
                      "type": "action",
                      "meta": {
                        "label": "New Receipt",
                        "icon": "add",
                        "actionType": "PRIMARY",
                        "handler": {
                          "handlerType": "NAVIGATION",
                          "action": "CREATE_NEW",
                          "params": {
                            "targetPage": "receiptCreate"
                          }
                        }
                      },
                      "properties": {}
                    },
                    {
                      "id": "create_shipment_action",
                      "version": 1,
                      "type": "action",
                      "meta": {
                        "label": "New Shipment",
                        "icon": "add",
