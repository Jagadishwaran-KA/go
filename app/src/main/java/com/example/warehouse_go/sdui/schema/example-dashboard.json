                      "version": 1,
                      "type": "action",
                      "meta": {
                        "label": "Reports",
                        "icon": "info",
                        "actionType": "TEXT",
                        "handler": {
                          "handlerType": "NAVIGATION",
                          "action": "NAVIGATE_TO",
                          "params": {
                            "targetPage": "reports"
                          }
                        }
                      },
                      "properties": {}
                    }
                  ]
                },
                "properties": {
                  "padding": "MEDIUM"
                }
              }
            ]
          },
          "properties": {}
        }
