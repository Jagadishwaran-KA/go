          "version": 1,
          "type": "container",
          "meta": {
            "arrangement": "COLUMN",
            "spacing": "SMALL",
            "scrollable": false,
            "children": [
              {
                "id": "quick_actions_title",
                "version": 1,
                "type": "text",
                "meta": {
                  "text": "Quick Actions",
                  "textContent": {
                    "text": "Quick Actions",
                    "textSize": "LARGE",
                    "fontWeight": "BOLD",
                    "textAlign": "START"
                  }
                },
                "properties": {
                  "padding": "MEDIUM"
